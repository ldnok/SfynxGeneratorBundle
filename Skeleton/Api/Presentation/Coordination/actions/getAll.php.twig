    /**
     * @return Response
     */
    public function getAllAction()
    {
        $adapter = new GetAllQueryAdapter();
        $query = $adapter->createQueryFromRequest(new GetAllRequest($this->request, $this->resolver));

        $entities = $this->getAllQueryHandler->process($query);

        return $this->responseHandler->create($entities, Response::HTTP_OK)->getResponse();
    }
